[user]
	name = mischizzle
	email = michelle@picot.ca

[alias]
	ci = commit
	st = status
	co = checkout
	br = branch
	lg = log -p
	stage = add
	unstage = reset HEAD
	plog = log --pretty=tformat:'%h %Cblue%cr%Creset %cn %Cgreen%s%Creset'

[color]
  ui = auto

[color "branch"]
  current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[push]
 	default = current

[apply]
	whitespace = nowarn

[core]
	editor = vim
	autocrlf = input
	safecrlf = true
	excludesfile = /Users/mimi/.global_ignore